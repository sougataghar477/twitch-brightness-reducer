{
  "manifest_version": 3,
  "name": "Twitch Brightness Reducer",
  "version": "4.0",
  "description": "Controls brightness of the videoplayer in Twitch.",
  "permissions": [
    "activeTab", "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "js": ["reducer.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_specific_settings": {
  "gecko": {
    "id": "{e68a0cd8-b588-45c7-ace5-e880df455533}"
  }
}

}
